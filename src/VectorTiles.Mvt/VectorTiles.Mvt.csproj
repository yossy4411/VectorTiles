<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>Okayu.VectorTiles.Mvt</PackageId>
        <Version>0.1.0</Version>
        <Copyright>Copyright (c) 2024 Okayu Group</Copyright>
        <RepositoryUrl>https://github.com/yossy4411/VectorTiles</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
        <PackageTags>Mapbox</PackageTags>
        <Authors>yossy4411</Authors>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Description>A library to read Mapbox Vector Tiles (MVT).</Description>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Google.Protobuf" Version="3.28.3" />
      <PackageReference Include="Okayu.VectorTiles.Core" Version="0.1.0" />
    </ItemGroup>

    <ItemGroup>
        <None Include="README.md" Pack="true" PackagePath="\"/>
    </ItemGroup>

</Project>
